<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <welcome-file-list>
        <welcome-file>Places</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>Places</servlet-name>
        <servlet-class>com.univ.servlets.Places</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Places</servlet-name>
        <url-pattern>/Places</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreatePlace</servlet-name>
        <servlet-class>com.univ.servlets.CreatePlace</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreatePlace</servlet-name>
        <url-pattern>/CreatePlace</url-pattern>
    </servlet-mapping>
</web-app>

